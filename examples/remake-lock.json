{
  "rules": [
    {
      "target": "main",
      "dependencies": [
        "foo.o",
        "math.o"
      ],
      "build_commands": [
        "gcc -g foo.o math.o -o main"
      ]
    },
    {
      "target": "clean",
      "dependencies": [],
      "build_commands": [
        "rm foo.o math.o main"
      ]
    },
    {
      "target": "foo.o",
      "dependencies": [
        "foo.c"
      ],
      "build_commands": [
        "gcc -g -c foo.c -o foo.o"
      ]
    },
    {
      "target": "math.o",
      "dependencies": [
        "math.c"
      ],
      "build_commands": [
        "gcc -g -c math.c -o math.o"
      ]
    }
  ],
  "wildcards": [
    {
      "symbol": "CC",
      "values": [
        "gcc"
      ]
    },
    {
      "symbol": "CFLAGS",
      "values": [
        "-g"
      ]
    },
    {
      "symbol": "CLEAN_FILES",
      "values": [
        "foo.o",
        "main"
      ]
    },
    {
      "symbol": "OBJS",
      "values": [
        "foo.o",
        "math.o"
      ]
    },
    {
      "symbol": "BIN",
      "values": [
        "main"
      ]
    }
  ]
}